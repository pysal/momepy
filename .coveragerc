[run]
source=momepy

[report]
exclude_lines =
    if self.debug:
    pragma: no cover
    raise NotImplementedError
    if __name__ == .__main__.:
    except ModuleNotFoundError:
    print("rtd")
    except ImportError
ignore_errors = True
omit =
    tests/*
    momepy/_version.py
    versioneer.py
    setup.py
    docs/conf.py
    benchmarks/*
